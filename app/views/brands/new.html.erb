<h1>New Brand</h1>


<%= form_tag(brands_url, method: 'post') do %>


  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

  <div>
    <%= label_tag :location %><br />
    <%= text_field_tag :location %>
  </div>


  <div>
    <%= submit_tag "Create Brand" %>
  </div>
<% end %>
